ForwardX11 yes
XAuthLocation /opt/X11/bin/xauth

Host github
    User git
    HostName github.com
    AddKeysToAgent yes
    #UseKeychain yes
    IdentityFile ~/.ssh/id_github
    Protocol 2

Host bitbucket
    User git
    HostName bitbucket.org
    AddKeysToAgent yes
    #UseKeychain yes
    IdentityFile ~/.ssh/id_bitbucket
    Protocol 2

Host 140.109.*
    ForwardAgent yes
