class DLien:
    def __init__(self):
        print( "I'm pig!" )

    import pudb; pudb.set_trace()  # XXX BREAKPOINT
    def chop(self):
        print( "It's not me." )

    def call_father(self):
        for i in range(3):
            import pudb; pudb.set_trace()  # XXX BREAKPOINT
            print( "Father! Help me! " )

if __name__ == '__main__':
    import pudb; pudb.set_trace()  # XXX BREAKPOINT
    pigLien = DLien()
    pigLien.chop()
    pigLien.call_father()
    print( 'Hello world.' )
